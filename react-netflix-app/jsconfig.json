{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES6",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["./components/*"],
      "@pages/*": ["./pages/*"],
      "@styles/*": ["./styles/*"],
      "@assets/*": ["./assets/*"],
      "@icons/*": ["./assets/icons/*"],
      "@api/*": ["./api/*"],
      "@constants/*": ["./constants/*"],
      "@utils/*": ["./utils/*"]
    }
  },
  "include": ["src"]
}
